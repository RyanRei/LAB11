#1
class traffic_light:
	def __init__(self,red_light,yellow_light,green_light):
		self.red_light = red_light
		self.yellow_light = yellow_Light
		self.green_light = green_Light
		
traffic_rules = traffic_light('Stop','Slow down','Go')
print(traffic_light.red_light)


#2
import math

class point:
	def __init__(self,x,y):
		self.x = x
		self.y = y
	def other_point(self,dx,dy):
		self.dx = dx
		self.dy = dy 
	def distance(self,other):
		dx = self.x - other.x
		dy = self.y - other.y
		return math.sqrt(dx**2 + dy**2)
